{
  "name": "GRMSBase-client",
  "version": "2.0.0",
  "scripts": {
    "start": "ng serve -c dev --host $npm_package_config_start_host --disable-host-check --proxy-config proxy.conf.json",
    "start:en": "ng serve -c dev-en --host $npm_package_config_start_host --disable-host-check --proxy-config proxy.conf.json",
    "start:ja": "ng serve -c dev-ja --disable-host-check --port 4300",
    "doc": "./node_modules/.bin/esdoc",
    "dev:build": "ng build --output-path=./dist",
    "dev:copy": "mkdir -p dist; cp -R assets dist/assets",
    "i18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json en ja",
    "build": "ng build -c production",
    "build:ja": "ng build -c ja",
    "clean": "rimraf dist",
    "test": "ng test",
    "lint": "ng lint",
    "lintbe": "tslint **/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "config": {
    "start": {
      "host": "localhost"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.2",
    "@angular/cdk": "^15.1.2",
    "@angular/common": "^15.1.2",
    "@angular/compiler": "^15.1.2",
