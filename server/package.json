{
  "name": "grms-server",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "./node_modules/.bin/run-p dev:nest dev:cpx",
    "dev:nest": "nest start --watch --debug",
    "dev:cpx": "cpx \"./src/**/**/dto*.ts\" ../client/src/api-dto --watch",
    "dev:migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate migration/development/Enhance -d src/app.datasource-mysql.ts",
    "dev:migration:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -d src/app.datasource-mysql.ts",
    "dev:dbinit": "./node_modules/.bin/run-s dev:migration:generate dev:migration:run",
    "migration:generate": "export IMDBTARGET=Migration && ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate migration/production/Enhance -d src/app.datasource-mysql.ts",
    "migration:run": "export IMDBTARGET=Migration && ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -d src/app.datasource-mysql.ts",
    "prebuild": "run-p clean",
    "build:cpx": "cpx \"./src/**/**/dto.*.ts\" ../client/src/api-dto",
    "build": "nest build",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "~9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "~9.0.3",
    "@nestjs/jwt": "~9.0.0",
    "@nestjs/passport": "~9.0.0",
    "@nestjs/platform-express": "~9.0.3",
    "@nestjs/platform-socket.io": "~9.0.3",
    "@nestjs/swagger": "~6.0.4",
    "@nestjs/typeorm": "~9.0.0",
    "@nestjs/websockets": "^9.0.3",
    "@notionhq/client": "^2.2.3",
    "archiver-zip-encryptable": "^1.0.10",
    "aws-sdk": "~2.1176.0",
    "axios": "^1.2.2",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concat-files-cli": "^0.1.6",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^2.0.0",
    "date-format": "^4.0.14",
    "dateformat": "^5.0.3",
    "generate-password": "^1.7.0",
    "googleapis": "^105.0.0",
    "jconv": "^0.1.5",
    "jest": "^28.1.3",
    "mailcomposer": "^4.0.2",
    "mailparser": "^3.5.0",
    "mongodb": "^5.3.0",
    "mysql2": "^2.3.3",
    "ngx-bootstrap": "^9.0.0",
    "nodemailer": "^6.7.7",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "pdfmake": "^0.2.5",
    "puppeteer": "^15.4.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "smtp-server": "^3.11.0",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "^28.0.7",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.3",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cpx": "^1.5.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "run-p": "0.0.0",
    "supertest": "^6.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
